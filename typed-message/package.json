{
  "name": "typed-message",
  "description": "A TypeScript and React library for providing type-safe internationalization messages.",
  "keywords": [
    "react",
    "typescript",
    "i18n",
    "internationalization",
    "vite",
    "type-safe",
    "messages",
    "formatting"
  ],
  "author": "Kouji Matsui (@kekyo@mi.kekyo.net)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/kekyo/typed-message.git"
  },
  "homepage": "https://github.com/kekyo/typed-message#readme",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./vite": {
      "types": "./dist/vite-plugin.d.ts",
      "import": "./dist/vite-plugin.mjs",
      "require": "./dist/vite-plugin.js"
    }
  },
  "files": [
    "dist",
    "README_pack.md",
    "images/typed-message-120-c.png"
  ],
  "scripts": {
    "build": "vite build",
    "test": "npm run build && vitest run",
    "prepack": "cp ../README_pack.md . && mkdir -p ./images && cp ../images/typed-message-120-c.png ./images/",
    "pack": "npm run build && npm run prepack && screw-up pack --pack-destination ../artifacts"
  },
  "peerDependencies": {
    "json5": ">=2.0.0",
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0",
    "vite": ">=5.0.0"
  },
  "devDependencies": {
    "@swc/core": ">=1.3.100",
    "@testing-library/react": ">=16.3.0",
    "@types/node": ">=20.0.0",
    "@types/react": ">=18.0.0",
    "@types/react-dom": ">=18.0.0",
    "@vitejs/plugin-react-swc": ">=3.5.0",
    "jsdom": ">=23.0.0",
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0",
    "screw-up": ">=0.12.0",
    "typescript": ">=5.0.0",
    "vite": ">=6.3.5",
    "vite-plugin-dts": ">=4.5.4",
    "vitest": ">=1.6.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}